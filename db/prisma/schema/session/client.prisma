model SessionClient {
  oid BigInt @id
  id  String @unique

  hash       String
  identifier String
  name       String

  payload Json

  tenantOid BigInt
  tenant    Tenant @relation(fields: [tenantOid], references: [oid])

  createdAt          DateTime            @default(now())
  sessionConnections SessionConnection[]

  @@unique([tenantOid, hash])
}
