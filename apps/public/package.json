{
  "name": "@metorial-subspace/app-public",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start:dev:server": "bun --watch ./src/server.ts",
    "start:dev:vite": "bun run dev",
    "start:dev": "bun run start:dev:server",
    "dev": "vite build --watch ./frontend",
    "dev:hmr": "vite ./frontend",
    "start": "bun ./src/server.ts",
    "build": "vite build ./frontend",
    "frontend:build": "vite build ./frontend",
    "server:build": "ncc build ./src/server.ts -o ./dist --source-map --external bun:sqlite --external bun && bun run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org metorial --project mte-subspace-frontend ./dist && sentry-cli sourcemaps upload --org metorial --project mte-subspace-frontend ./dist"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.957.0",
    "@aws-sdk/client-iam": "^3.957.0",
    "@aws-sdk/client-lambda": "^3.958.0",
    "@hono/zod-validator": "^0.7.6",
    "@hookform/resolvers": "^5.2.2",
    "@lowerdeck/api-key": "^1.0.1",
    "@lowerdeck/api-mux": "^1.0.1",
    "@lowerdeck/error": "^1.0.10",
    "@lowerdeck/hono": "^1.0.6",
    "@lowerdeck/rpc-server": "^1.0.8",
    "@lowerdeck/validation": "^1.0.4",
    "@metorial-io/data-hooks": "^1.0.1",
    "@metorial-io/pages": "^1.0.0",
    "@metorial-subspace/app-controller": "workspace:*",
    "@metorial-subspace/module-auth": "^1.0.0",
    "@lowerdeck/telemetry": "^1.1.1",
    "@metorial-subspace/module-tenant": "^1.0.0",
    "@metorial-subspace/presenters": "^1.0.0",
    "@sentry/bun": "^10.34.0",
    "@sentry/cli": "^3.2.0",
    "@types/react-dom": "^19.2.3",
    "@vercel/ncc": "^0.38.3",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "semver": "^7.7.3",
    "snowflake-uuid": "^1.0.0",
    "styled-components": "^6.1.13",
    "unzipper": "^0.12.3",
    "vite": "^5.4.1",
    "yaml": "^2.8.2",
    "yup": "^1.7.1"
  }
}
